<%- messages() %>
<h1>Account Management</h1>

<h2>Welcome, <%= accountData.account_firstname %></h2>

<% if (accountData.account_type === "Employee" || accountData.account_type === "Admin") { %>
    <h3>Inventory Management</h3>
    <p><a href="/inv" title="Manage inventory">Go to Inventory Management</a></p>
<% } %>

<p><a href="/account/update/<%= accountData.account_id %>" title="Update your account information">Update Account Information</a></p>